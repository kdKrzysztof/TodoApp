{
  "semi": true,
  "tabWidth": 2,
  "printWidth": 100,
  "singleQuote": true,
  "trailingComma": "none",
  "jsxBracketSameLine": true,
  "importOrder": [
    "^src",
    "^components",
    "^components/(.*)$",
    "^utils",
    "^utils/(.*)$",
    "^pages",
    "^pages/(.*)$",
    "^[./]",
    "^types"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
